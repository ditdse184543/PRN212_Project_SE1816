<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BadmintonWPFApp"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"

        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" x:Class="BadmintonWPFApp.WindowDashBoard"
        mc:Ignorable="d"
        Title="WindowDashBoard" Height="450" Width="800">
    <Grid>
        <lvc:CartesianChart Name="cartesianChart">
            <lvc:CartesianChart.Series>
                <lvc:ColumnSeries Title="2024" Values="{Binding Values2024}" />
                <lvc:ColumnSeries Title="2025" Values="{Binding Values2025}" />
            </lvc:CartesianChart.Series>
            <lvc:CartesianChart.AxisX>
                <lvc:Axis Title="Month" Labels="{Binding Labels}" />
            </lvc:CartesianChart.AxisX>
            <lvc:CartesianChart.AxisY>
                <lvc:Axis Title="Sales" LabelFormatter="{Binding Formatter}" />
            </lvc:CartesianChart.AxisY>
        </lvc:CartesianChart>
    </Grid>
</Window>
