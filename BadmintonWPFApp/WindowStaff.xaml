<Window x:Class="BadmintonWPFApp.WindowStaff"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BadmintonWPFApp"
        mc:Ignorable="d"
        Title="WindowStaff" Height="450" Width="800">
    <Grid>
        <DockPanel>
            <Button DockPanel.Dock ="Top" Margin="10" Padding="10" Click="Button_Checkin">
                Checkin timeslot
            </Button>
            <Button DockPanel.Dock ="Top" Margin="10" Padding="10" Click="Button_Report">
                Report court quality
            </Button>
            <!--<Button DockPanel.Dock ="Top" Margin="10" Padding="10" Click="Button_Log_Out">
                Log out
            </Button>-->
        </DockPanel>
    </Grid>
</Window>
